<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Overtime App Notification Alert", sticky: false) %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: "Overtime App Notification Error", sticky: false) %>
<% else %>
  <%= js add_gritter(flash[:notice], title: "Overtime App Notification Notice", sticky: false) %>
<% end %>